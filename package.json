{
  "name": "@webrsb/nestjs-oauth2-server",
  "version": "1.1.0",
  "description": "A NestJS wrapper for node-oauth/oauth2-server",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "B'Tunde Aromire",
  "keywords": [
    "nestjs oauth2 server",
    "nestjs oauth2",
    "@nestjs/oauth2-server",
    "nest oauth2 server",
    "nestjs",
    "nest"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "format": "prettier --write lib/**/*.ts tests/**/*.ts",
    "prebuild": "rimraf -rf dist",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rimraf -rf dist && tsc -p tsconfig.json -w",
    "lint": "eslint lib/**/*.ts --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "npm run test:integration",
    "test:integration": "jest --config ./tests/jest-e2e.config.json --runInBand",
    "test:cov": "jest --config ./tests/jest-e2e.config.json --runInBand --coverage",
    "prerelease": "npm run build",
    "release": "dotenv release-it --"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@node-oauth/oauth2-server": "^5.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "17.0.24",
    "@types/rimraf": "3.0.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "5.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lint-staged": "12.3.8",
    "moment": "2.29.4",
    "prettier": "^3.0.0",
    "reflect-metadata": "0.1.13",
    "release-it": "14.14.2",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "supertest": "6.2.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webrsb/nestjs-oauth2-server.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webrsb/nestjs-oauth2-server/issues"
  },
  "peerDependencies": {
    "@node-oauth/oauth2-server": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "homepage": "https://github.com/webrsb/nestjs-oauth2-server#readme",
  "publishConfig": {
    "registry": "https://nexus.athena.com.tw/repository/npm-host/"
  }
}
